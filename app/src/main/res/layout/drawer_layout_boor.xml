        <ImageView
            android:id="@+id/image_tx"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:src="@drawable/aa" />

        <TextView
            android:id="@+id/text_mz"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_alignTop="@+id/image_tx"
            android:layout_marginLeft="55dp"
            android:layout_marginStart="55dp"
            android:layout_toEndOf="@+id/image_tx"
            android:layout_toRightOf="@+id/image_tx"
            android:gravity="center"
            android:text="King-国王"
            android:textSize="20dp" />

        <ImageView
            android:id="@+id/imageVip"
            android:layout_width="50dp"
            android:layout_height="30dp"
            android:layout_alignBottom="@+id/image_tx"
            android:layout_alignLeft="@+id/text_mz"
            android:layout_alignStart="@+id/text_mz"
            android:src="@drawable/qz_xinxi" />

        <TextView
            android:id="@+id/text_vip"
            android:layout_width="50dp"
            android:layout_height="30dp"
            android:layout_alignBottom="@+id/image_tx"
            android:layout_alignLeft="@+id/text_mz"
            android:layout_alignStart="@+id/text_mz"
            android:gravity="center"
            android:text="LV.5" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_alignTop="@+id/imageVip"
            android:layout_toEndOf="@+id/imageVip"
            android:layout_toRightOf="@+id/imageVip"
            android:src="@drawable/xingbie_nv" />

    </RelativeLayout>

